{
    "name": "dukecity/command-scheduler-bundle",
    "description": "This Symfony bundle will allow you to schedule all your commands just like UNIX crontab",
    "keywords": ["Symfony", "scheduler", "cron", "command", "task"],
    "homepage": "https://github.com/Dukecity/CommandSchedulerBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Julien Guyon",
            "email": "julienguyon@hotmail.com"
        },
        {
            "name": "Dukecity",
            "email": "dukecity.code@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0.2",
        "symfony/console": "^4.4.31 || ^5.3 || ^6.0",
        "doctrine/orm": "^2.9",
        "doctrine/doctrine-bundle": "^2.4.2",
        "doctrine/collections": "^1.6.8",
        "doctrine/dbal": "^2.13.3",
        "doctrine/doctrine-migrations-bundle": "^3.1.0",
        "doctrine/persistence": "^2.2.2",
        "laminas/laminas-code": "4.4",
        "dragonmantank/cron-expression": "^3.1",
        "symfony/framework-bundle": "^4.4.31 || ^5.3 || ^6.0",
        "symfony/twig-bundle": "^4.4.31 || ^5.3 || ^6.0",
        "symfony/translation": "^4.4.31 || ^5.3 || ^6.0",
        "symfony/finder": "^4.4.31 || ^5.3 || ^6.0",
        "symfony/form": "^4.4.31 || ^5.3 || ^6.0",
        "symfony/asset": "^4.4.31 || ^5.3 || ^6.0",
        "symfony/validator": "^4.4.31 || ^5.3 || ^6.0",
        "nesbot/carbon": "^2.53",
        "symfony/dotenv": "^4.4.31 || ^5.3 || ^6.0",
        "symfony/lock": "^4.4.31 || ^5.3 || ^6.0",
        "sensio/framework-extra-bundle": "^6.2",
        "symfony/config": "^4.4.31 || ^5.3 || ^6.0",
        "symfony/dependency-injection": "^4.4.31 || ^5.3 || ^6.0",
        "symfony/property-access": "^4.4.31 || ^5.3 || ^6.0",
        "symfony/http-kernel": "^4.4.31 || ^5.3 || ^6.0",
        "symfony/routing": "^4.4.31 || ^5.3 || ^6.0",
        "symfony/yaml": "^4.4.31 || ^5.3 || ^6.0",
        "symfony/var-dumper": "^4.4.31 || ^5.3 || ^6.0",
        "twig/twig": "^3.3",
        "lorisleiva/cron-translator": "^0.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.9",
        "symfony/phpunit-bridge": "^4.4.31 || ^5.3 || ^6.0",
        "doctrine/doctrine-fixtures-bundle": "^3.4.0",
        "symfony/debug-bundle": "^4.4.31 || ^5.3 || ^6.0",
        "symfony/dom-crawler": "^4.4.31 || ^5.3 || ^6.0",
        "symfony/css-selector": "^4.4.31 || ^5.3 || ^6.0",
        "liip/test-fixtures-bundle": "^2.1.0",
        "symfony/browser-kit": "^4.4.31 || ^5.3 || ^6.0",
        "symfony/maker-bundle": "^1.33",
        "jetbrains/phpstorm-attributes": "^1.0"
    },
    "suggest": {
        "ext-pcntl": "For using the scheduler daemon",
        "symfony/notifier": "Send Notifications (E-Mails) ^4.4.31 || ^5.3 || ^6.0",
        "twig/cssinliner-extra": "Needed for E-Mail Notification ^3.0",
        "twig/inky-extra": "Needed for E-Mail Notification ^3.0",
        "ext-posix": "*"
    },
    "autoload": {
        "psr-4": {
            "Dukecity\\CommandSchedulerBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "Tests/"
        }
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^4.4.31 || ^5.3 || ^6.0"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": false
}
